{
  "coc.preferences.formatOnSaveFiletypes": ["json", "vim", "fish"],
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "coc-actions.useCursorLine": true,
  "coc-actions.showActionKind": true,
  "coc-actions.hideCursor": true,
  "tsserver.enable": true,
  "suggest.snippetIndicator": "\u26a1 ",
  "codeLens.enable": true,
  "diagnostic.virtualText": true,
  "diagnostic.errorSign": "◉",
  "suggest.completionItemKindLabels": {
    "module": " ",
    "function": "ﬦ",
    "variable": "ﮂ"
  }
}
